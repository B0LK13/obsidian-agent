# Vibe.d Development Environment Configuration
# Copy this file to .env and customize as needed

# Application
ENVIRONMENT=development
LOG_LEVEL=debug

# Database
DATABASE_URL=postgresql://vibed:vibed_secret@localhost:5432/vibed_dev
POSTGRES_USER=vibed
POSTGRES_PASSWORD=vibed_secret
POSTGRES_DB=vibed_dev

# Redis
REDIS_URL=redis://localhost:6379/0

# D Compiler Settings
DUB_COMPILER=dmd
DUB_BUILD_FLAGS=-debug

# Server
SERVER_PORT=8080
SERVER_HOST=0.0.0.0
